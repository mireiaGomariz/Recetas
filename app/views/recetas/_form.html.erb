<h1>Receta#new</h1>

<%= form_for @receta, url: {action: "create"} do |f| %>

  <%= f.label :titulo %><br>
  <%= f.text_field :titulo %><br>

  <%= f.label :ingredientes %><br>
  <%= f.text_area :ingredientes %><br>

  <%= f.label :elaboracion %><br>
  <%= f.text_area :elaboracion %><br>


  <%= f.submit "Subir Receta"%>

<% end %>
